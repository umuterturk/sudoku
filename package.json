{
  "name": "sudoku-game",
  "version": "1.0.0",
  "description": "A simple Sudoku game built with React",
  "main": "index.js",
  "scripts": {
    "dev": "npx vite",
    "build": "npx vite build && node optimize-build.js",
    "build:analyze": "npx vite build --mode analyze && node optimize-build.js",
    "build:prod": "npm run build",
    "preview": "npx vite preview",
    "deploy": "npm run build && gh-pages -d dist",
    "optimize": "node optimize-build.js",
    "firebase:init": "firebase init firestore",
    "firebase:deploy": "firebase deploy --only firestore:rules",
    "firebase:login": "firebase login"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "firebase": "^12.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.8.2"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "gh-pages": "^6.1.1",
    "terser": "^5.43.1",
    "vite": "^5.1.0"
  },
  "keywords": [
    "sudoku",
    "game",
    "react"
  ],
  "author": "",
  "license": "MIT"
}
